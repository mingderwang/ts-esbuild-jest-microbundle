{
  "name": "test1-ts-esbuild-ava",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && node ./scripts/build.js",
    "build:node": "yarn clean && node ./scripts/build-node.js",
    "start": "node ./dist/app.js",
    "test": "jest",
    "clean": "rimraf ./dist",
    "fix": "cd node_modules && ln -s esbuild-wasm esbuild"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "esbuild-jest": "^0.5.0",
    "esbuild-wasm": "^0.14.51",
    "jest": "^28.1.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts": "^0.2.2"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "tsconfig.json"
  ],
  "keywords": [
    "template",
    "typescript",
    "microbundle",
    "esbuild",
    "ava"
  ],
  "ava": {
    "typescript": {
      "rewritePaths": {
        "src/": "build/"
      },
      "compile": false
    }
  }
}
