{
  "name": "ts-esbuild-jest-microbundle",
  "version": "0.0.1",
  "private": true,
  "source": "src/main.ts",
  "main": "dist/main.umd.js",
  "module": "dist/main.modern.module.js",
  "unpkg": "dist/main.umd.js",
  "types": "dist/main.d.ts",
  "license": "MIT",
  "scripts": {
    "build:test": "node ./scripts/build-test.js",
    "build": "microbundle",
    "build:react": "yarn clean && node ./scripts/build.js",
    "build:node": "yarn clean && node ./scripts/build-node.js",
    "start": "node ./dist/app.js",
    "test": "jest",
    "test:ava": "ava src/main.test.ts",
    "clean": "rimraf ./dist",
    "prepare": "cd node_modules && ln -s esbuild-wasm esbuild"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ava/typescript": "^3.0.1",
    "@types/jest": "^28.1.6",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "ava": "^4.3.1",
    "esbuild-jest": "^0.5.0",
    "esbuild-wasm": "^0.14.51",
    "jest": "^28.1.3",
    "microbundle": "^0.15.0",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts": "^0.2.2",
    "ts-node": "^10.9.1"
  },
  "engines": {
    "node": ">=14"
  },
  "files": [
    "tsconfig.json"
  ],
  "keywords": [
    "template",
    "typescript",
    "microbundle",
    "esbuild",
    "ava"
  ],
  "ava": {
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  }
}
